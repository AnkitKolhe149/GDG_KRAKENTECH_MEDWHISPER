{
  "buildCommand": "pip install -r requirements.txt",
  "outputDirectory": ".",
  "framework": "flask",
  "env": {
    "PYTHON_VERSION": "3.13"
  },
  "functions": {
    "wsgi.py": {
      "memory": 3008,
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/wsgi.py"
    }
  ],
  "regions": ["iad1"],
  "installCommand": "pip install -r requirements.txt"
}
